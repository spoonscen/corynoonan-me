{
  "name": "corynoonan-me",
  "version": "0.0.1",
  "description": "Personal website of Cory Noonan @spoonscen",
  "main": "index.js",
  "scripts": {
    "production": "webpack && npm start",
    "concurrently": "concurrently -p command",
    "coverage-report": "open coverage/lcov-report/index.html",
    "coveralls": "npm run test -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "dev": "npm run concurrently --kill-others 'npm run frontend' 'npm run server'",
    "frontend": "webpack --watch",
    "server": "nodemon --watch 'api/**/*.ts' --ignore 'src/**/*' --exec 'ts-node' api/server.ts",
    "start": "'ts-node' api/server.ts",
    "test": "ts-node node_modules/istanbul/lib/cli.js cover --include-all-sources true -e .ts -e .tsx -x '*.spec.*' -x webpack.config.ts _mocha -- '{api,src}/**/*.spec.{ts,tsx}' --require ignore-styles --require test/setup-env.js"
  },
  "author": "Cory E Noonan",
  "license": "ISC",
  "dependencies": {
    "bluebird": "3.4.6",
    "express": "4.14.0",
    "history": "4.4.0",
    "lodash": "4.17.2",
    "path": "0.12.7",
    "react": "15.3.2",
    "react-copy-to-clipboard": "4.2.3",
    "react-dom": "15.3.2",
    "react-router": "3.0.0",
    "react-tooltip": "^3.2.2",
    "request": "2.79.0",
    "request-promise": "4.1.1"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/enzyme": "2.5.38",
    "@types/express": "4.0.34",
    "@types/history": "2.0.39",
    "@types/lodash": "4.14.40",
    "@types/mocha": "2.2.33",
    "@types/nock": "8.2.0",
    "@types/react": "0.14.47",
    "@types/react-dom": "0.14.18",
    "@types/react-router": "2.0.38",
    "@types/request-promise": "^4.1.33",
    "@types/sinon": "1.16.32",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "concurrently": "3.1.0",
    "coveralls": "^2.11.15",
    "css-loader": "0.26.0",
    "enzyme": "2.6.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "ignore-styles": "5.0.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "9.0.2",
    "node-sass": "3.13.0",
    "react-addons-test-utils": "15.3.2",
    "remap-istanbul": "^0.7.0",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "1.1.0",
    "ts-node": "^1.7.0",
    "tslint": "4.0.2",
    "tslint-react": "2.0.0",
    "typescript-require": "0.2.9-1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3"
  }
}
